{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "your-connection-string"
  },
  "Fingerprint": {
    "RateLimitThreshold": 100,
    "RateLimitWindow": "00:15:00",
    "WhitelistedIps": [
      "127.0.0.1",
      "::1"
    ]
  },
  "AllowedHosts": "*"
}